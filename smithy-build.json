{
    // Version of the smithy-build.json file specification
    "version": "1.0",
    // Location to search for Smithy model source files
    "sources": ["model"],
    "maven": {
        "dependencies": [
            // Used to get smithy protocols
            "software.amazon.smithy:smithy-aws-traits:1.53.0",
            // Used to generated a typescript client
            "software.amazon.smithy.typescript:smithy-aws-typescript-codegen:0.25.0"
        ]
    },
    "plugins": {
        "typescript-codegen": {
            "package": "@weather-service/client",
            "packageVersion": "0.0.1"
        }
    }
}
